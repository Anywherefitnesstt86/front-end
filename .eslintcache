earchTerm(searchTerm);\n    // setSearchTerm({searchTerm: e.target.value}); // cleaner option\n    // console.log(\"The changeHandler is running.\")\n    getFilteredClasses(searchTerm);\n  };\n  \n  // -------------------- Side Effects -----------------\n\n  \n\n\n  return (\n    <div className='classes-background'>\n      <div className='classes-content-container'>\n        \n        <div className='search-container'><br/>\n          <div className='classes-title'>          \n            <h1>Classes</h1>\n          </div>\n\n          <SearchIcon className='search-icon' style={{ color: 'white'}} fontSize=\"large\"/>\n          <TextInput\n              type=\"text\"\n              placeholder=\" Search for classes\"\n              onChange={searchChangeHandler} \n              label={\"\"}\n          />\n        </div>\n        \n\n        <div className=\"classes-container\">\n            {\n              filteredClasses.map(indivClass => {\n                return <Class key={indivClass.id} indivClass={indivClass} />\n              })\n            }\n        </div>\n      </div>\n    </div>\n  )\n}","/Users/qirhi/Desktop/Lambda/lambda-code/lambda-unit-two-projects/anywhere-fitness/src/Components/Class.js",[],"/Users/qirhi/Desktop/Lambda/lambda-code/lambda-unit-two-projects/anywhere-fitness/src/Components/Footer.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":1,"column":8,"nodeType":"65","messageId":"66","endLine":1,"endColumn":12},{"ruleId":"63","severity":1,"message":"67","line":6,"column":8,"nodeType":"65","messageId":"66","endLine":6,"endColumn":13},{"ruleId":"63","severity":1,"message":"68","line":8,"column":8,"nodeType":"65","messageId":"66","endLine":8,"endColumn":14},{"ruleId":"63","severity":1,"message":"69","line":55,"column":9,"nodeType":"65","messageId":"66","endLine":55,"endColumn":29},{"ruleId":"63","severity":1,"message":"70","line":77,"column":10,"nodeType":"65","messageId"